<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane prefHeight="184.0" prefWidth="260.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.example.informationsystem.controller.AddOrderController">
    <GridPane layoutX="14.0" layoutY="14.0" prefHeight="129.0" prefWidth="232.0">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="105.0" minWidth="10.0" prefWidth="71.0"/>
            <ColumnConstraints halignment="LEFT" hgrow="SOMETIMES" maxWidth="159.0" minWidth="10.0" prefWidth="148.0"/>
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        </rowConstraints>
        <TextField fx:id="price" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
        <Label text="Date"/>
        <Label text="Customer" GridPane.rowIndex="1"/>
        <Label text="Price" GridPane.rowIndex="2"/>
        <DatePicker fx:id="date" editable="false" GridPane.columnIndex="1"/>
        <ComboBox fx:id="combo" onAction="#onActionComboBox" onShowing="#OnShowingComboBox" prefHeight="25.0"
                  prefWidth="110.0" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
        <Button alignment="CENTER_RIGHT" contentDisplay="RIGHT" mnemonicParsing="false"
                onAction="#onAddCustomerClick" prefHeight="25.0" prefWidth="38.0" text="Add"
                GridPane.columnIndex="1" GridPane.halignment="RIGHT" GridPane.rowIndex="1"/>
    </GridPane>
    <Button fx:id="addOrder" layoutX="104.0" layoutY="143.0" mnemonicParsing="false" onAction="#onAddOrderButtonClick"
            text="Add"/>
</AnchorPane>
