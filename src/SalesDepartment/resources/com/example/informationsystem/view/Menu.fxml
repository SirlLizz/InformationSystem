<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.RowConstraints?>

<VBox alignment="CENTER" prefHeight="408.0" prefWidth="562.0" spacing="20.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" >
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <children>
        <MenuBar>
            <menus>
                <Menu mnemonicParsing="false" text="File">
                    <items>
                        <Menu mnemonicParsing="false" text="Order">
                            <items>
                      <MenuItem mnemonicParsing="false" fx:id="serializeOrderButton" text="Serialize Order" />
                        <MenuItem mnemonicParsing="false" fx:id="openOrderButton" text="Open Order" />
                        <MenuItem mnemonicParsing="false" fx:id="addOpenOrderButton" text="Open to Add Order" />
                            </items>
                        </Menu>
                        <Menu mnemonicParsing="false" text="Customer">
                            <items>
                    <MenuItem mnemonicParsing="false" fx:id="serializeCustomerButton" text="Serialize Customer" />
                        <MenuItem mnemonicParsing="false" fx:id="openCustomerButton" text="Open Customer" />
                        <MenuItem mnemonicParsing="false" fx:id="addOpenCustomerButton" text="Open to Add Customer" />
                            </items>
                        </Menu>
                    </items>
                </Menu>
            </menus>
        </MenuBar>
        <HBox prefHeight="283.0" prefWidth="417.0">
            <children>
              <TextArea fx:id="customer" prefHeight="278.0" prefWidth="330.0" />
            <TextArea fx:id="order" prefHeight="278.0" prefWidth="330.0" />
            </children>
        </HBox>
        <GridPane prefHeight="30.0" prefWidth="197.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            </rowConstraints>
            <children>
              <Button fx:id="addCustomerButton" prefHeight="25.0" prefWidth="264.0" text="Add Customer" />
              <Button fx:id="addOrderButton" alignment="CENTER" contentDisplay="RIGHT" mnemonicParsing="false" prefHeight="25.0" prefWidth="263.0" text="Add Order" textAlignment="CENTER" GridPane.columnIndex="1" />
          </children>
       </GridPane>
   </children>
</VBox>
